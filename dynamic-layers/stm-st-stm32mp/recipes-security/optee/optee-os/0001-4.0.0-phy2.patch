+		regulator@10 {
+			reg = <VOLTD_SCMI_STPMIC2_BUCK7>;
+			voltd-supply = <&v3v3>;
+		};
+		regulator@15 {
+			reg = <VOLTD_SCMI_STPMIC2_LDO5>;
