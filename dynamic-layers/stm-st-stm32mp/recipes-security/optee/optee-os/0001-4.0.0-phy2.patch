+			voltd-supply = <&vdd3v3_usb>;
+		};
+		regulator@17 {
+			reg = <VOLTD_SCMI_STPMIC2_LDO7>;
+			voltd-supply = <&vdd_sdcard>;
+		};
+	};
+};
